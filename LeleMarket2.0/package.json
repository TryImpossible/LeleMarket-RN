{
  "name": "LeleMarket",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cross-env ENVFILE=.env.development && react-native run-android --variant devDebug --appId com.lelemarket.dev",
    "ios": "cross-env ENVFILE=.env.development && react-native run-ios --configuration DevDebug --scheme LeleMarket-Dev",
    "start": "cross-env ENVFILE=.env.development && react-native start --verbose",
    "start:display": "cross-env ENVFILE=.env.development && METRO_VARIANT=display react-native start --verbose",
    "start:clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn start -- --reset-cache",
    "dll": "webpack-cli --config ./web/config/webpack.dll.config.js --progress --inline --colors",
    "web": "webpack-dev-server --config ./web/config/webpack.config.development.js --progress --inline --colors",
    "build:web": "webpack-cli --config ./web/config/webpack.config.production.js --progress --inline --colors",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write src/**",
    "tsc": "tsc --noEmit",
    "image": "node local-cli/generator/imagescript.js",
    "svg": "node local-cli/generator/svgscript.js"
  },
  "dependencies": {
    "@react-native-community/hooks": "^2.6.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/viewpager": "^4.1.6",
    "@react-navigation/bottom-tabs": "^5.9.1",
    "@react-navigation/native": "^5.7.5",
    "@react-navigation/stack": "^5.9.2",
    "axios": "^0.20.0",
    "crypto-js": "^3.1.9-1",
    "dayjs": "^1.9.1",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "0.64.1",
    "react-native-config": "^1.3.3",
    "react-native-device-info": "^6.2.0",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-motion": "^1.0.5",
    "react-native-read-more-text": "^1.1.2",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-shared-element": "^0.7.0",
    "react-native-svg-uri": "^1.2.3",
    "react-native-web": "^0.13.16",
    "react-native-webview": "^10.9.2",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/axios": "^0.14.0",
    "@types/crypto-js": "^3.1.47",
    "@types/jest": "^25.2.2",
    "@types/lodash": "^4.14.151",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-native": "^0.62.9",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "^3.6.0",
    "@types/redux-immutable": "^4.0.1",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-native-web": "^0.12.2",
    "babel-plugin-root-import": "^6.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "colors": "^1.4.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react-hooks": "^4.0.4",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "^25.1.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.1.3",
    "metro-react-native-babel-preset": "^0.59.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.13.1",
    "redux-devtools": "^3.5.0",
    "style-loader": "^1.1.4",
    "thread-loader": "^2.1.3",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "uglifyjs-webpack-plugin": "1",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "git add --force"
    ],
    "*.{js,jsx,ts,tsx,json,md,graphql}": [
      "yarn prettier",
      "git add --force"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  }
}
